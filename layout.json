    "layout:widgetTMTCBootCause": {
        "composition": [
            "beacon.telemetry:boot_cause"
        ],
        "name": "Cause of last reboot",
        "type": "summary-widget",
        "modified": "1",
        "location": "layout:nomTMTC",
        "configuration": {
            "ruleOrder": [
                "default",
                "off"
            ],
            "ruleConfigById": {
                "default": {
                    "name": "default",
                    "label": "On",
                    "message": "",
                    "id": "default",
                    "icon": "",
                    "style": {
                        "color": "#ffffff",
                        "background-color": "#8DB600",
                        "border-color": "rgba(0,0,0,0)"
                    },
                    "conditions": [
                        {
                            "object": "beacon.telemetry:boot_cause",
                            "key": "value",
                            "operation": "",
                            "values": [
                                1
                            ]
                        }
                    ],
                    "jsCondition": "",
                    "trigger": "any",
                    "expanded": "true"
                },
                "off": {
                    "name": "off",
                    "label": "Off",
                    "message": "",
                    "id": "off",
                    "icon": "icon-alert-triangle",
                    "style": {
                        "color": "ffffff",
                        "background-color": "#D3212D",
                        "border-color": "rgba(0,0,0,0)"
                    },
                    "conditions": [
                        {
                            "object": "beacon.telemetry:boot_cause",
                            "key": "value",
                            "operation": "lessThan",
                            "values": [
                                2
                            ]
                        }
                    ],
                    "jsCondition": "",
                    "trigger": "any",
                    "expanded": "true"
                }
            },
            "testDataConfig": [
                {
                    "object": "",
                    "key": "",
                    "value": ""
                }
            ]
        }
